<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".presentation.myPage.MyPageFragment">

    <!-- TODO: Update blank fragment layout -->

    <ImageView
        android:id="@+id/img_myPage"
        android:layout_width="120dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="120dp"/>

    <TextView
        android:id="@+id/btn_getImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이미지 불러오기"
        app:layout_constraintEnd_toEndOf="@+id/img_myPage"
        app:layout_constraintStart_toStartOf="@+id/img_myPage"
        app:layout_constraintTop_toBottomOf="@id/img_myPage"/>

</androidx.constraintlayout.widget.ConstraintLayout>